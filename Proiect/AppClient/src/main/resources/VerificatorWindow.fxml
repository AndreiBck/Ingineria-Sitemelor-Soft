<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<StackPane minWidth="400.0" maxWidth="400.0" maxHeight="250.0" minHeight="250.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.121" fx:controller="gui.VerificatorController">
        <children>
            <StackPane>
                <children>
                    <VBox spacing="10.0">
                        <children>
                            <GridPane>
                                <children>
                                    <Label text="Name:" GridPane.columnIndex="0" GridPane.rowIndex="0" />
                                    <Label text="Description:" GridPane.columnIndex="0" GridPane.rowIndex="1" />
                                    <TextField fx:id="name" promptText="name" GridPane.columnIndex="1" GridPane.rowIndex="0" />
                                    <TextField fx:id="description" promptText="description" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                                </children>
                                <columnConstraints>
                                    <ColumnConstraints hgrow="SOMETIMES" maxWidth="148.0" minWidth="10.0" prefWidth="109.0" />
                                    <ColumnConstraints hgrow="SOMETIMES" maxWidth="228.0" minWidth="10.0" prefWidth="189.0" />
                                </columnConstraints>
                                <rowConstraints>
                                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                                </rowConstraints>
                            </GridPane>
                            <StackPane prefHeight="-1.0" prefWidth="-1.0">
                                <children>
                                    <Button fx:id="loginButton" alignment="CENTER" defaultButton="true" mnemonicParsing="false" onAction="#pressAddBug" text="Add Bug" StackPane.alignment="CENTER_RIGHT" />
                                </children>
                            </StackPane>
                        </children>
                    </VBox>
                </children>
            </StackPane>
        </children>
        <padding>
            <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
        </padding>
    </StackPane>