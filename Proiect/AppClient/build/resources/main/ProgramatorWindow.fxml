<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.*?>
<AnchorPane prefHeight="350.0" prefWidth="200.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.ProgramatorController">

    <children>

        <ListView  fx:id="listaBugs" layoutX="29.0" layoutY="161.0" prefHeight="250.0" prefWidth="400.0" AnchorPane.leftAnchor="50" AnchorPane.topAnchor="50">
        </ListView>

        <GridPane AnchorPane.leftAnchor="500" AnchorPane.topAnchor="100" maxWidth="500.0">
            <children>
                <Button fx:id="ShowAllBtn" alignment="CENTER" defaultButton="true" mnemonicParsing="false" onAction="#pressShowAllBtn" text="Show all bugs" GridPane.rowIndex="0"/>
                <Button fx:id="SolveBugBtn" alignment="CENTER" defaultButton="true" mnemonicParsing="false" onAction="#pressSolveBugButton" text="Solve bug" GridPane.rowIndex="1"/>
            </children>
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="120.0" minWidth="20.0" prefWidth="100.0" />
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="120.0" minWidth="20.0" prefWidth="100.0" />
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0"  vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            </rowConstraints>
        </GridPane>

    </children>

</AnchorPane>